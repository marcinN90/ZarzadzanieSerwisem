<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@ViewBag.Title - Aplikacja do zarządzania serwisem</title>
    <meta http-equiv="content-type" content="text/html;charset=utf-8" />
    @Styles.Render("~/Content/css")

</head>
<body>

    <div id="menuContainer">
        <div class="centerContainer">
            <ul id="nav">
                <li>@Html.ActionLink("Start", "Index", "Start")</li>                          
                <li>@Html.ActionLink("DEMO", "Index", "PanelStart")</li>
                <li>@Html.ActionLink("Kontakt", "Kontakt", "Start")</li>
        </div>
    </div>
    @RenderBody()
  
    <div id="footerCopyrightContainer">
        <div class="centerContainer">
            <p class="fl">&copy; Copyright @System.DateTime.Today.Year by Marcin Nowak</p>
          
        </div>
    </div>
    <script type="text/javascript">Cufon.now();</script>
